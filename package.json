{
  "name": "the-terminal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:analyze": "ANALYZE=true next build",
    "build:linux": "rm -rf .next && bun run next build && cp -rf ./public/. ./.next/standalone/public && cp -rf ./.next/static/. ./.next/standalone/.next/static",
    "build": "bun i && next build",
    "build:windows": "bun run next build && xcopy \".\\public\" \".\\.next\\standalone\\public\" /e /i /y && xcopy \".\\.next\\static\" \".\\.next\\standalone\\.next\\static\" /e /i /y",
    "dev": "next dev --port 5188",
    "lint": "bun run format:fix && next lint",
    "start": "PORT=5188 node server.js",
    "start:dev:linux": "bun i && bun run build:linux && cd .next/standalone && bun start",
    "start:dev:windows": "bun i && bun run build:windows && cd \".\\.next\\standalone\" && bun start",
    "format": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "deps:update": "npx npm-check-updates --interactive --format group",
    "type-check": "npx tsc --noEmit",
    "list:unused-dependencies": "bunx depcheck"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.3.4",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.3.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.14",
